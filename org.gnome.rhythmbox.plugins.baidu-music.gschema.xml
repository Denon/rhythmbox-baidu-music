<schemalist>
    <schema id="org.gnome.rhythmbox.plugins.baidu-music" path="/org/gnome/rhythmbox/plugins/baidu-music/">
        <key name="username" type="s">
            <default>''</default>
            <summary>Username</summary>
            <description>The username of baidu music account.</description>
        </key>
        <key name="password" type="s">
            <default>''</default>
            <summary>Password</summary>
            <description>The password of baidu music account.</description>
        </key>
        <key name="lyric-path" type="s">
            <default>'~/.lyrics/'</default>
            <summary>Lyric dir</summary>
            <description>The dir of lyrics</description>
        </key>
        <key name="hq" type="b">
            <default>false</default>
            <summary>HQ music</summary>
            <description>the status of HQ music</description>
        </key>
        <child name="source" schema="org.gnome.rhythmbox.source"/>
    </schema>
</schemalist>
